                          <a href="/users/sign_in">로그인</a>
                        <% end %>
                                <!-- Modal -->
                        <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                          <div class="modal-dialog" role="document">
                            <div class="modal-content">
                              <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                  <span aria-hidden="true">&times;</span>
                                </button>
                              </div>
                                    
                         <form action="/body" method="post" id="modalsubmit">
                            <div class="modal-body" style="padding-top:0;">
                              
                             <div id="checkboxs" class="form-check row" style="text-align: center;">
                                  <label class="checkboxs col-12">
                                    <input type="checkbox" name="shoulder" value="1" class="invisible">
                                      <strong>어깨</strong>
                                  </label>
                                  <label class="checkboxs col-12">
                                    <input type="checkbox" name="back" value="2" class="invisible">
                                      <strong>허리</strong>
                                  </label>
                                  <label class="checkboxs col-12">
                                    <input type="checkbox" name="pelvis" value="3" class="invisible">
                                      <strong>골반</strong>
                                  </label>
                                  <label class="checkboxs col-12">
                                    <input type="checkbox" name="leg" value="4" class="invisible">
                                      <strong>다리</strong>
                                  </label>
                                  <label class="checkboxs col-12">
                                    <input type="checkbox" name="foot" value="5" class="invisible">
                                      <strong>발</strong>
                                  </label>
                              </div>  
                              
                            </div>
                          </form>
                              
                              <div class="modal-footer">
                                 <button type="submit" class="btn btn-block btn-round" style="background-color: #F4646A;" form="modalsubmit">
                                      <i class="fa fa-check" aria-hidden="true" style="color: #fff;"></i>
                                 </button>
                              </div>
                            </div>
                          </div>
                        </div>
                        
                        <script>
                        (function(){
                          var checkboxs = document.getElementById("checkboxs");
                        
                          checkboxs.addEventListener("click", function(e){
                            var target = e.target || e.srcElement;
                            target.classList.toggle("is_checked");
                          },true);
                        
                        })();
                        </script>


                    </div>
                </div>
